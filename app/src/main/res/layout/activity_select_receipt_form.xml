<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".SelectReceiptFormActivity">

    <include layout="@layout/activity_toolbar" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="147dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight=".5"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/iv_sevenEleven"
                android:layout_width="57.17dp"
                android:layout_height="59dp"
                android:layout_centerInParent="true"
                android:background="@drawable/rectangle_border"
                android:src="@drawable/seven" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/iv_sevenEleven"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:paddingBottom="15dp"
                android:text="เซเว่น อีเลเว่น" />
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight=".5"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/iv_makro"
                android:layout_width="100dp"
                android:layout_height="60dp"
                android:background="@drawable/rectangle_border"
                android:layout_centerInParent="true"
                android:src="@drawable/makro" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/iv_makro"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:paddingBottom="15dp"
                android:text="แม็คโคร" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="147dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight=".5"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/iv_tesco"
                android:layout_width="57.17dp"
                android:layout_height="59dp"
                android:layout_centerInParent="true"
                android:background="@drawable/rectangle_border"
                android:src="@drawable/tesco_lotus" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/iv_tesco"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:paddingBottom="15dp"
                android:text="เทสโก้ โลตัส" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight=".5"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/iv_bigc"
                android:layout_width="57.17dp"
                android:layout_height="59dp"
                android:layout_centerInParent="true"
                android:background="@drawable/rectangle_border"
                android:src="@drawable/bic_c" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/iv_bigc"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:paddingBottom="15dp"
                android:text="บิ๊กซี" />
        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="147dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight=".5"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/iv_family"
                android:layout_width="57.17dp"
                android:layout_height="59dp"
                android:layout_centerInParent="true"
                android:background="@drawable/rectangle_border"
                android:src="@drawable/family" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/iv_family"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:paddingBottom="15dp"
                android:text="แฟมิลี่ มาร์ท" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight=".5"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/iv_tops"
                android:layout_width="57.17dp"
                android:layout_height="59dp"
                android:layout_centerInParent="true"
                android:background="@drawable/rectangle_border"
                android:src="@drawable/tops" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/iv_tops"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:paddingBottom="15dp"
                android:text="ท็อปส์ มาร์เก็ท" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="147dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight=".5"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/iv_central_food"
                android:layout_width="57.17dp"
                android:layout_height="59dp"
                android:layout_centerInParent="true"
                android:background="@drawable/rectangle_border"
                android:src="@drawable/central_food" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/iv_central_food"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:paddingBottom="15dp"
                android:text="เซ็นทรัล ฟู้ด ฮอล์" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight=".5"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/iv_other"
                android:layout_width="57.17dp"
                android:layout_height="59dp"
                android:layout_centerInParent="true"
                android:background="@drawable/rectangle_border"
                android:src="@drawable/other" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/iv_other"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:paddingBottom="15dp"
                android:text="ใบเสร็จอื่นๆ" />
        </RelativeLayout>
    </LinearLayout>

